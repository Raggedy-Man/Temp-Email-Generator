{
  "manifest_version": 2,

  "name": "Temporary Email Generator (by fsociety)",
  "description": "This extension allows the user to copy a temp email or just directly go into the inbox.",
  "version": "1.0",
  "applications":{
  	"gecko":{
  		"strict_min_version":"54.0a1"
  	}
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "click me!"
  },

  "permissions":[
    "activeTab",
    "storage"
  ],

  "commands":{
  	"_execute_browser_action":{
  		"suggested_key":{
  			"default":"Ctrl+Shift+Y"
  		}
  	}
  }
}
